<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div id="readRequest">
    <div class="sep-space-sm"></div>

    <div class="readrequest-basic-information block-padding">
      <div class="row">
        <div class="col-xs-5 col-sm-4 p0m0">
          <div class="col-xs-6 col-sm-4 col-md-4 newrequest-title text-left refId">Ref ID</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divRefID"><%= request.ReferenceID %></div>
        </div>

        <div class="col-xs-7 col-sm-7 p0m0">
          <div class="col-xs-6 col-sm-4 col-md-4 newrequest-title text-left">Create Date</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divCreateddate"><%= request.CreatedOn %></div>
        </div>
      </div>
      <div class="sep-space-sm"></div>

      <div class="row">
        <div class="col-xs-12 col-sm-4 p0m0">
          <div class="col-xs-4 col-sm-4 col-md-4 newrequest-title text-left">Status</div>
          <div class="col-xs-8 col-sm-8 col-md-8" id="divStatus">
            <%= request.DisplayStatus %>
          </div>
          <div class="sep-space-sm clear"></div>
        </div>

        <div class="col-xs-12 col-sm-7 p0m0">
          <div class="col-xs-4 col-sm-4 col-md-4 newrequest-title text-left">Prepared by</div>
          <div class="col-xs-8 col-sm-8 col-md-8 " id="divPreparedby"><%= request.PreparerFNAME %></div>
        </div>
      </div>
      <div class="sep-space-sm"></div>
    </div>
    <div class="sep-space-sm"></div>


    <div class="select-applicant block-padding">
      <div class="row">
        <div class="col-xs-4 col-sm-2 col-md-2 newrequest-title text-left">Applicant</div>
        <div class="col-xs-8 col-sm-10 col-md-10 btn-group full-width p-r-0 applicant-container form_data">
          <%= request.ApplicantFNAME%>
        </div>
      </div>
      <div class="sep-space-sm"></div>

      <div class="row">
        <div class="col-xs-4 col-sm-2 col-md-2 newrequest-title text-left">Department</div>
        <div class="col-xs-8 col-sm-10 col-md-10" id="divdepartment"><%= request.DEPT %></div>
      </div>
      <div class="sep-space-sm"></div>

    </div>
    <div class="sep-space-sm"></div>


    <div class="request-application">
      <div class="sep-space-sm"></div>
      <div class="block-padding">
        <div class="newrequest-title text-left">Service Acquired for</div>
      </div>
      <div class="select-service-header" style="border: none;">
        <div class="">
          <div id="service-container">
            <!-- ========================= -->
            <!-- service type content here -->
            <!-- ========================= -->
          </div>

          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">
              <span><span class='red'>* </span>Justification and Important Notes</span>
              <span class="service-title-desp">(Business Value / Descripttion / Reason)</span>
            </div>
          </div>
          <div class="row field">
            <div class="col-md-10 form_data">
              <% if (request.Justification) { %>
                <%= request.Justification %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Expected Delivery Date</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <% if (request.EDeliveryDate) { %>
                <%= request.EDeliveryDate %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Frequency/Duration of Use</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <% if (request.DurationOfUse) { %>
                <%= request.DurationOfUse %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left"><span class='red'>* </span>Estimated Cost (HK$)</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <% if (request.EstimatedCost) { %>
                <%= request.EstimatedCost %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Budget Provided</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <% if (request.BudgetProvided) { %>
                <%= request.BudgetProvided %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>
          <!-- <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Budget Sum</div>
          </div>
          <div class="row field">
            <div class="col-xs-10 col-sm-10">
              <input type="text" class="form-control" id="txtbudgetsum" field="BudgetSum" value="<%= request.BudgetSum %>"/>
            </div>
          </div>
          <div class="sep-space-sm"></div> -->

          <div class="row field">
            <div class="col-xs-12 newrequest-title text-left"><span class='red'>* </span>Recommend By</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 btn-group full-width recommend-container form_data">
              <% if (request.ApproverFNAME) { %>
                <%= request.ApproverFNAME %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 newrequest-title text-left">CC</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 contact-group"></div>
          </div>
          <div class="sep-space"></div>

          <!-- <div class="row field">
            <div class="col-xs-10 col-sm-2 newrequest-title text-left">Remark</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 form_data">
              <% if (request.Remark) { %>
                <%= request.Remark %>
              <% } else { %>
                NIL
              <% } %>
            </div>
          </div>
          <div class="sep-space"></div> -->


          <div class="rowrow field" id="attachment-container">
            <!-- attachment here -->
          </div>
          <div class="sep-space"></div>

          <div class="rowrow field" id="workflowlog-container">
            <!-- workflow log here -->
          </div>
          <div class="sep-space"></div>

          <% if (request.showRemark) { %>

            <div class="sep-space-db-border"></div>
            <div class="sep-space"></div>
            <div class="row field">
              <div class="col-xs-10 col-sm-2 newrequest-title text-right mobile-text-align-left text-lg-right">Remark</div>
              <div class="col-xs-12 col-sm-10 col-md-8 form_data">
                <textarea class="form-control keepEdit" rows="3" id="txtRemark" field="Remark"></textarea>
              </div>
            </div>

            <div class="sep-space"></div>

            <% if (request.showForwardTo) { %>
              <div class="row field">
                <div class="col-xs-10 col-sm-2 newrequest-title text-right mobile-text-align-left text-lg-right" style='line-height:34px'>Forward To</div>
                <div class="col-xs-12 col-sm-10 col-md-8 form_data forwardToUser">
                </div>
              </div>
            <% } %>



            <div class="sep-space"></div>
            <div class="sep-space-db-border"></div>

          <% } %>

          <div class="row field">
            <div class="col-xs-10 col-sm-2 newrequest-title"></div>
            <div class="col-xs-12 col-md-3 btn-group" style="text-align: left;"></div>

          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 buttons-container">
              <!--
                <button class="btn btn-primary" id="btnBack" style="display:none" type="button">Back</button>
                <button class="btn btn-primary" id="btnsave" type="button">Save</button>
                <button class="btn btn-primary" id="btnapprover" type="button">Send to Approver</button>
              -->
            </div>
          </div>

        </div>
      </div>
    </div>

  </div>
</div>
