<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div id="readRequest">

    <div class="readrequest-basic-information">
      <div class="row">
        <div class="col-xs-12 col-sm-5 p0m0">
          <div class="col-xs-1 col-sm-2 newrequest-title text-left text-lg-right refId">Ref ID</div>
          <div class="col-xs-4 col-sm-8 col-md-2" id="divRefID"><%= request.ReferenceID %></div>
        </div>

        <div class="col-xs-12 col-sm-6 p0m0">
          <div class="col-xs-1 col-sm-2 col-md-2 newrequest-title  text-left text-lg-right">Create Date</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divCreateddate"><%= request.CreatedOn %></div>
        </div>
      </div>
      <div class="sep-space"></div>

      <div class="row">
        <div class="col-xs-12 col-sm-5 p0m0">
          <div class="col-xs-1 col-sm-2 newrequest-title text-left text-lg-right">Status</div>
          <div class="col-xs-8 col-sm-6" id="divStatus">
            <%= request.FormStatus %>
          </div>
        </div>

        <div class="col-xs-12 col-sm-6 p0m0">
          <div class="col-xs-1 col-sm-2 newrequest-title text-left text-lg-right">Prepared by</div>
          <div class="col-xs-8 col-sm-8 " id="divPreparedby"><%= request.PreparerFNAME %></div>
        </div>
      </div>
    </div>
    <div class="sep-space"></div>


    <div class="select-applicant">
      <div class="row">
        <div class="col-xs-2 col-sm-1 newrequest-title text-left">Applicant *</div>
        <div class="col-xs-8 col-sm-10 col-md-8 btn-group full-width p-r-0 applicant-container form_data">
          <%= request.selectedApplicantName%>
        </div>
      </div>

      <div class="row">
        <div class="col-xs-2 col-md-2 newrequest-title text-left">Title</div>
        <div class="col-xs-10 col-md-10" id="divtitle"><%= request.Title %></div>
      </div>

      <div class="row">
        <div class="col-xs-12 col-md-2 newrequest-title text-left">Office</div>
        <div class="col-xs-12 col-md-10" id="divoffice"><%= request.Location %></div>
      </div>

      <div class="row">
        <div class="col-xs-2 col-sm-1 newrequest-title text-left">Department</div>
        <div class="col-xs-8 col-sm-10 col-md-8 p-r-0" id="divdepartment"><%= request.DEPT %></div>
      </div>
    </div>
    <div class="sep-space"></div>


    <div class="request-application">
      <div class="row">
        <div class="col-xs-10 col-md-10 newrequest-title text-left pl10 pb10">Service Acquired for</div>
      </div>
      <div class="select-service-header" style="border: none;">
        <div class="">
          <div id="service-container">
            <!-- ========================= -->
            <!-- service type content here -->
            <!-- ========================= -->
          </div>

          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Justification and Important Notes *
              <span class="service-title-desp">(Business Value / Descripttion / Reason)</span>
            </div>
          </div>
          <div class="row field">
            <div class="col-md-10 form_data">
              <%= request.Justification %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Expected Delivery Date</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <%= request.EDeliveryDate %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Frequency/Duration of Use</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <%= request.DurationOfUse %>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Estimated Cost (HK$) *</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <%= request.EstimatedCost %>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Budget Provided</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_data">
              <%= request.BudgetProvided %>
            </div>
          </div>
          <div class="sep-space"></div>
          <!-- <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Budget Sum</div>
          </div>
          <div class="row field">
            <div class="col-xs-10 col-sm-10">
              <input type="text" class="form-control" id="txtbudgetsum" field="BudgetSum" value="<%= request.BudgetSum %>"/>
            </div>
          </div>
          <div class="sep-space"></div> -->

          <div class="row field">
            <div class="col-xs-12 newrequest-title text-left">Recommend By *</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 btn-group full-width recommend-container form_data">
              <%= request.ApproverFNAME %>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 newrequest-title text-left">CC</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 contact-group"></div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-10 col-sm-2 newrequest-title text-left">Remark</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 col-md-8 form_data">
              <%= request.Remark %>
            </div>
          </div>
          <div class="sep-space"></div>


          <div class="rowrow field" id="attachment-container">
            <!-- attachment here -->
          </div>
          <div class="sep-space"></div>

          <div class="rowrow field" id="workflowlog-container">
            <!-- workflow log here -->
          </div>
          <div class="sep-space"></div>

          <% if (request.showComment) { %>

            <div class="sep-space"></div>
            <div class="row field">
              <div class="col-xs-10 col-sm-2 newrequest-title text-right mobile-text-align-left text-lg-right">Comment</div>
              <div class="col-xs-12 col-sm-10 col-md-8 form_data">
                <textarea class="form-control keepEdit" rows="3" id="txtcomment" field="Comment"></textarea>
              </div>
            </div>

          <% } %>

          <div class="row field">
            <div class="col-xs-10 col-sm-2 newrequest-title"></div>
            <div class="col-xs-12 col-md-3 btn-group" style="text-align: left;"></div>

          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 buttons-container">
              <!--
                <button class="btn btn-primary" id="btnBack" style="display:none" type="button">Back</button>
                <button class="btn btn-primary" id="btnsave" type="button">Save</button>
                <button class="btn btn-primary" id="btnapprover" type="button">Send to Approver</button>
              -->
            </div>
          </div>

        </div>
      </div>
    </div>

  </div>
</div>
