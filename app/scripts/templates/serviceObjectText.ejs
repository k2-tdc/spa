<% if (!serviceObject.readonly) { %>
  <span class="service-title-desp" style="margin:0">
    <%= serviceObject.Name %>
  </span>
  <textarea class="level2 form-control lastnosub" id="lastnosub" rows="3" placeholder="<%= serviceObject.Placeholder %>"><%= serviceObject.SValue %></textarea>
<%} else {%>
  <div class="service-title-desp">
    <%= serviceObject.Name %>
  </div>
  <div class="service-content">
    <%= serviceObject.SValue %>
  </div>
  <div class='sep-space-sm'></div>
<%}%>
