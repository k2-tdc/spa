<div class="panel panel-default">
  <div class="panel-title panel-heading subheader-menu-container headingOne">
    <!-- subheader menu here -->
  </div>

  <div id="newRequest">

    <div class="newrequest-basic-information">
      <div class="sep-space-sm"></div>
      <div class="row">
        <!--<div class="col-xs-4 col-sm-5 p0m0">
			<% if (request.ReferenceID) { %>
			  <div class="col-xs-1 col-sm-2 newrequest-title text-left text-lg-right refId">Ref ID</div>
			  <div class="col-xs-4 col-sm-8 col-md-2" id="divRefID"><%= request.ReferenceID %></div>
			<% } else { %>
			  <div class="col-xs-0 col-sm-2 newrequest-title text-left text-lg-right" ></div>
			  <div class="col-xs-7 col-sm-2 col-md-2" id="divRefID" ></div>
			<% } %>
		</div>-->


        <div class="col-xs-5 col-sm-4 p0m0">
          <div class="col-xs-6 col-sm-4 col-md-4 newrequest-title text-left refId">Ref ID</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divRefID"><%= request.ReferenceID %></div>
        </div>

        <div class="col-xs-7 col-sm-7 p0m0">
          <div class="col-xs-6 col-sm-4 col-md-4 newrequest-title text-left">Create Date</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divCreateddate"><%= request.CreatedOn %></div>
        </div>

      </div>

      <div class="row">
        <div class="col-xs-12 col-sm-4 p0m0">
          <div class="col-xs-6 col-sm-4 col-md-4 newrequest-title text-left">Status</div>
          <div class="col-xs-6 col-sm-8 col-md-8" id="divStatus">
            <% if (request.DisplayStatus) { %>
              <%= request.DisplayStatus %>
            <% } else { %>
              New
            <% } %>
          </div>
        </div>

        <div class="col-xs-12 col-sm-7 p0m0">
          <div class="col-xs-4 col-sm-4 col-md-4 newrequest-title text-left">Prepared by</div>
          <div class="col-xs-8 col-sm-8 col-md-8 " id="divPreparedby"><%= request.PreparerFNAME %></div>
        </div>
      </div>
    </div>
    <div class="sep-space-sm"></div>

    <div class="panel panel-default select-applicant">
      <div class="panel-body">
        <div class="row">
          <div class="col-xs-2 col-sm-2 text-left newrequest-title">Applicant</div>
          <div class="col-xs-8 col-sm-10 col-md-8 btn-group full-width p-r-0 applicant-container form_input">
            <!-- <button type="button" class="btn btn-default btnapplicant selectedApplicant" field="selectedApplicant" data-toggle="dropdown">
              <% if (request.selectedApplicantName){ %>
                <%= request.selectedApplicantName%>
              <% } else { %>
                --Select--
              <% } %>
            </button>
            <button type="button" class="btn btn-default dropdown-toggle btnapplicant" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
              <span class="caret"></span>
              <span class="sr-only">Toggle Dropdown</span>
            </button> -->
            <!-- applicant list here -->
            <div class="error-message hidden">Please choose one</div>

          </div>
        </div>
        <div class="row" style="display:none">
          <div class="col-xs-2 col-md-2 newrequest-title text-left">Title</div>
          <div class="col-xs-10 col-md-10" id="divtitle"><%= request.Title %></div>
        </div>
        <div class="row" style="display:none">
          <div class="col-xs-12 col-md-2 newrequest-title text-left">Office</div>
          <div class="col-xs-12 col-md-10" id="divoffice"><%= request.Location %></div>
        </div>
        <div class="sep-space"></div>
        <div class="row">
          <div class="col-xs-2 col-sm-2 newrequest-title text-left">Department</div>
          <div class="col-xs-8 col-sm-10 col-md-8 p-r-0" id="divdepartment"><%= request.DEPT %></div>
        </div>
      </div>
    </div>
    <div class="sep-space"></div>
    <div class="request-application">
      <div class="row">
        <div class="col-xs-10 col-md-10 newrequest-title text-left pl10 pb10">Service Acquired for</div>
      </div>
      <div class="select-service-header" style="border: none;">
        <div class="">
          <div id="service-container">
            <!-- ========================= -->
            <!-- service type content here -->
            <!-- ========================= -->
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">
              <span><span class='red'>* </span>Justification and Important Notes</span><br>
              <span class="service-title-desp" style='margin-left: 0;'>(Business Value / Description / Reason)</span>
            </div>
          </div>
          <div class="row field">
            <div class="col-md-10 form_input">
              <textarea class="form-control" rows="5" field="Justification" id="txtjustification"><%= request.Justification %></textarea>
              <div class="error-message hidden">Please fill up this field</div>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Expected Delivery Date</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_input">
              <div class="input-group">
                <input type="text" class="form-control date" id="txtexpectedDD" field="EDeliveryDate" value="<% if (request.EDeliveryDate) { %><%= request.EDeliveryDate %><% } %>"/>
                <div class="input-group-addon datepicker-toggle-btn">
                  <span class="glyphicon glyphicon-calendar"></span>
                </div>
              </div>
              <div class="error-message hidden">experted delivery date must be >= create date and today</div>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Frequency/Duration of Use</div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_input">
              <input type="text" class="form-control" id="txtfrequency" field="DurationOfUse" value="<%= request.DurationOfUse %>"/>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">
              <span><span class='red'>* </span>Estimated Cost (HK$)</span><br>
              <span class="service-title-desp" style='margin-left: 0;'>Please convert the estimated cost in HKD</span>

            </div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_input">
              <input type="number" class="form-control" id="txtestimatedcost" field="EstimatedCost" value="<%= request.EstimatedCost %>" />
              <div class="error-message hidden">Please fill up this field</div>
            </div>
          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">
              <span>Budget Provided</span><br>
              <span class="service-title-desp" style='margin-left: 0;'>
                Please specify the full budget code including:
              </span><br>
              <span class="service-title-desp" style='margin-left: 0;'>
                nature code, 5 digit; dept code, 2 digit; activities code, 6 digit; and fiscal year, 4 digit
              </span>
            </div>
          </div>
          <div class="row field">
            <div class="col-xs-12 col-sm-10 form_input">
              <textarea rows="5" class="form-control" id="txtbudgetprovided" field="BudgetProvided"><%= request.BudgetProvided %></textarea>
            </div>
          </div>
          <div class="sep-space"></div>
          <!-- <div class="row field">
            <div class="col-xs-12 col-md-12 newrequest-title text-left">Budget Sum</div>
          </div>
          <div class="row field">
            <div class="col-xs-10 col-sm-10">
              <input type="text" class="form-control" id="txtbudgetsum" field="BudgetSum" value="<%= request.BudgetSum %>"/>
            </div>
          </div>
          <div class="sep-space"></div> -->
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-12 col-sm-3 newrequest-title text-right mobile-text-align-left text-lg-right"><span class='red'>* </span>Recommend By</div>
            <div class="col-xs-12 col-sm-9 col-md-9 btn-group full-width form_input">
              <div class="recommend-container">
                Please select service and filled the cost field
              </div>
              <!-- <button type="button" class="btn btn-default recommend" field="selectedRecommentModel" data-toggle="dropdown" id="recommend-btn">
                <% if (request.ApproverFNAME) { %>
                  <%= request.ApproverFNAME %>
                <% } else { %>
                  --Select--
                <% } %>
              </button>
              <button type="button" class="btn btn-default dropdown-toggle recommend" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span class="caret"></span>
                <span class="sr-only">Toggle Dropdown</span>
              </button> -->
              <div class="error-message hidden">Please choose one</div>

              <!-- <ul id="ulrecommend" style="height:250px;overflow-y:scroll" class="dropdown-menu">
              </ul> -->

            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-10 col-sm-3 newrequest-title text-right mobile-text-align-left text-lg-right">CC</div>
            <div class="col-xs-12 col-sm-9 col-md-9 contact-group"></div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-2 col-sm-3 hidden-xs newrequest-title text-lg-right  ">&nbsp;</div>
            <div class="col-xs-12 col-sm-9 col-md-9 btn-group full-width cc-container form_input">
              <input type="text" class="cc-picker form-control" data-provide="typeahead" autocomplete="off"/>
              <!-- <button type="button" class="btn btn-default btncc selectedCC" data-toggle="dropdown">--Select--</button>
              <button type="button" class="btn btn-default dropdown-toggle btncc" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <span class="caret"></span>
                <span class="sr-only">Toggle Dropdown</span>
              </button> -->
              <!-- cc list here -->

            </div>
          </div>
          <div class="sep-space"></div>

          <div class="row field">
            <div class="col-xs-10 col-sm-3 newrequest-title text-right mobile-text-align-left text-lg-right">Remark</div>
            <div class="col-xs-12 col-sm-9 col-md-9 form_input">
              <textarea class="form-control" rows="3" id="txtremark" field="Remark"><%= request.Remark %></textarea>
            </div>
          </div>
          <div class="sep-space"></div>

          <div class="rowrow field" id="attachment-container">
            <!-- attachment here -->
          </div>
          <div class="sep-space"></div>

          <div class="rowrow field" id="workflowlog-container">
            <!-- workflow log here -->
          </div>
          <div class="sep-space"></div>

          <% if (request.showComment) { %>

            <div class="sep-space"></div>
            <div class="row field">
              <div class="col-xs-10 col-sm-3 newrequest-title text-right mobile-text-align-left text-lg-right">Comment</div>
              <div class="col-xs-12 col-sm-9 col-md-9 form_input">
                <textarea class="form-control keepEdit" rows="3" id="txtcomment" field="Comment"></textarea>
              </div>
            </div>

          <% } %>

          <div class="row field">
            <div class="col-xs-10 col-sm-2 newrequest-title"></div>
            <div class="col-xs-12 col-md-3 btn-group" style="text-align: left;"></div>

          </div>
          <div class="sep-space"></div>
          <div class="row field">
            <div class="col-xs-12 col-md-12 buttons-container">
              <!--
                <button class="btn btn-primary" id="btnBack" style="display:none" type="button">Back</button>
                <button class="btn btn-primary" id="btnsave" type="button">Save</button>
                <button class="btn btn-primary" id="btnapprover" type="button">Send to Approver</button>
              -->
            </div>
          </div>

        </div>
      </div>
    </div>

  </div>
</div>
